#!/bin/sh

if [ "$#" -eq 0 ]; 
then
  cd ~/corelx
else 
  cd ~/corelx/_"$1"/ >/dev/null 2>&1;

  if [ "$2" != "" ];
  then   
    if cd ./"$2" >/dev/null 2>&1;
    then
    elif cd ./comp"$2" >/dev/null 2>&1;
    then 
    elif cd ./seng"$2" >/dev/null 2>&1;
    then
    elif cd ./math"$2" >/dev/null 2>&1;
    then
    else
    echo -e "${Red} »» Failed to goto directory @ $1 -> $2."
    fi
  fi 
fi
